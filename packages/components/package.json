{
  "name": "@photonhealth/components",
  "private": "true",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "start": "vite --port 3000",
    "build": "vite build && npx tailwindcss -i ./src/index.css -o ./dist/style.css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes --allow-console-errors",
    "test": "vitest"
  },
  "type": "module",
  "dependencies": {
    "@photonhealth/sdk": "*",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "clsx": "^1.2.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jwt-decode": "^3.1.2",
    "solid-heroicons": "^3.1.1",
    "solid-transition-group": "^0.2.2",
    "superstruct": "^1.0.3"
  },
  "peerDependencies": {
    "solid-js": "1.6.11"
  },
  "engines": {
    "node": ">=16"
  }
}
